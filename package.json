{
  "name": "es6_brownbag",
  "author": "luc",
  "license": "MIT",
  "version": "1.0.0",
  "description": "ES6 refresher brownbag",
  "scripts": {
    "test": "NODE_ENV=test npx jest --runInBand",
    "lint": "npx eslint --ignore-path .gitignore './src/**/*.js'"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-optional-chaining": "7.9.0",
    "axios": "^0.19.2",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babelify-es6-polyfill": "1.0.4",
    "crypto-js": "3.1.9-1",
    "crypto-token": "1.0.1",
    "d3-format": "1.4.1",
    "data.task": "3.1.2",
    "jwt-decode": "2.2.0",
    "lodash": "4.17.15",
    "md5": "2.2.1",
    "node-fetch": "2.6.0",
    "reverse-geocoding": "3.0.2",
    "slug": "1.1.0",
    "validator": "11.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.6.4",
    "@babel/core": "7.6.4",
    "@babel/node": "7.6.3",
    "@babel/preset-env": "7.6.3",
    "@babel/preset-es2015": "7.0.0-beta.53",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "console.table": "0.10.0",
    "eslint": "6.6.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-json": "2.0.1",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-app": "6.2.2",
    "jest": "24.9.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "collectCoverage": false,
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "preset": "react-native",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/app/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "reporters": [
      "default",
      [
        "<rootDir>/node_modules/jest-html-reporter",
        {
          "pageTitle": "React Native Tests",
          "outputPath": "./reports/react-native-tests.html",
          "includeFailureMsg": true
        }
      ]
    ]
  }
}
